{
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "temp/",
    "esModuleInterop": true,
    "noImplicitAny": true,
    "sourceMap": false,
    "downlevelIteration": true,
    "module": "none",
    "moduleResolution": "node",
    "lib": ["esnext"],
    "target": "esnext"
  },
  "exclude": ["node_modules/**/*", "temp/**/*", "lib/**/*", "**/*.spec.ts"]
}
