{
  "name": "@iamsquare/complex.js",
  "version": "1.0.0",
  "description": "A simple complex numbers library written in Typescript",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/types/index.d.ts",
  "scripts": {
    "setup": "npm install",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage",
    "clean": "npm-run-all -p clean:*",
    "clean:lib": "rimraf ./lib",
    "clean:temp": "rimraf ./temp",
    "clean:docs": "rimraf ./docs",
    "clean:coverage": "rimraf ./coverage",
    "docs": "npm-run-all clean:docs docs:generate",
    "docs:generate": "typedoc --name complex.js --readme ./README.md --mode file --theme minimal --hidegenerator --excludeexternals --tsconfig ./tsconfig.json --out ./docs ./src",
    "build:watch": "tsc -w",
    "build:bundle": "rollup -c",
    "build:dist": "tsc -p ./tsconfig.d.json",
    "watch": "npm-run-all -p clean:lib clean:temp -s build:watch",
    "build": "npm-run-all -p clean:lib clean:temp -s build:dist build:bundle",
    "dev": "npm-run-all watch test:watch",
    "prod": "npm-run-all clean test:coverage build docs:generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iamsquare/complex.js.git"
  },
  "keywords": [
    "typescript",
    "javascript",
    "complex-numbers",
    "math",
    "library"
  ],
  "author": "squaremarco",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iamsquare/complex.js/issues"
  },
  "homepage": "https://github.com/iamsquare/complex.js",
  "private": false,
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@types/jest": "^24.0.0",
    "@types/node": "^10.12.24",
    "concurrently": "^4.1.0",
    "jest": "^24.1.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-terser": "^4.0.4",
    "serve": "^10.1.2",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "typedoc": "^0.14.2",
    "typescript": "^3.3.3"
  },
  "directories": {
    "lib": "lib",
    "test": "tests"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {}
}
