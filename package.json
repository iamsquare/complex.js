{
  "name": "@iamsquare/complex.js",
  "version": "0.3.1",
  "description": "A simple complex numbers library written in Typescript",
  "main": "lib/complex.js",
  "types": "lib/types/complex.d.ts",
  "scripts": {
    "setup": "npm install",
    "test": "jest",
    "test:coverage": "npm run clean:coverage && jest --coverage",
    "test:watch": "npm run test -- --watch",
    "clean:docs": "rimraf ./docs",
    "clean:coverage": "rimraf ./coverage",
    "clean:lib": "concurrently \"rimraf ./lib\" \"rimraf ./temp\"",
    "clean": "concurrently \"npm run clean:lib\" \"npm run clean:docs\" \"npm run clean:coverage\"",
    "docs": "npm run clean:docs && typedoc --name complex.js --readme ./README.md --mode file --theme minimal --hidegenerator --excludeexternals --tsconfig ./tsconfig.json --out ./docs ./src",
    "build": "npm run clean:lib && concurrently \"tsc\" \"tsc -p ./tsconfig.d.json\" && rollup -c",
    "build:watch": "npm run clean:lib && tsc -w",
    "start": "npm run build:watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iamsquare/complex.js.git"
  },
  "keywords": [
    "typescript",
    "javascript",
    "complex-numbers",
    "math",
    "library"
  ],
  "author": "squaremarco",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iamsquare/complex.js/issues"
  },
  "homepage": "https://github.com/iamsquare/complex.js",
  "private": false,
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.9.3",
    "concurrently": "^4.0.1",
    "jest": "^23.5.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.65.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-terser": "^2.0.2",
    "serve": "^10.0.0",
    "ts-jest": "^23.1.4",
    "typedoc": "^0.12.0",
    "typescript": "^3.0.3"
  },
  "directories": {
    "lib": "lib",
    "test": "tests"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {}
}
